<?xml version="1.0" encoding="UTF-8"?>
<g:Grid xmlns="http://www.butent.com/column" xmlns:g="http://www.butent.com/grid"
  
  name="TradeStatuses" viewName="TradeStatuses" caption="=trdStatuses"
  >
  <g:columns>
    <DataColumn name="StatusName" visible="true" minWidth="100" />
    <DataColumn name="StatusActive" visible="true" />

    <DataColumn name="Background" updateMode="row" minWidth="100">
      <editor type="color" />
    </DataColumn>
    <DataColumn name="Foreground" updateMode="row" minWidth="100">
      <editor type="color" />
    </DataColumn>

    <DataColumn name="PhaseOrder" />
    <DataColumn name="PhasePending" />
    <DataColumn name="PhaseActive" />
    <DataColumn name="PhaseCompleted" />
    <DataColumn name="PhaseApproved" />
  </g:columns>
</g:Grid>
