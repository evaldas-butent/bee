<?xml version="1.0" encoding="UTF-8"?>
<FlowPanel xmlns="http://www.butent.com/widget" class="flexBox-vertical">
  <FlowPanel name="Objects" 
    style="max-height: 20%; overflow: auto; margin-top: 1ex; margin-left: 1em;" />

  <HorizontalPanel style="margin: 2ex 1em;">
    <Label text="=object" style="padding-right: 1em;" />
    <UnboundSelector viewName="Locations" renderColumns="LocationName CompanyName"
      newRowEnabled="false" editPopup="true" style="width: 400px;" filter="Manager = {u}
      OR !(Manager)" />
  </HorizontalPanel>
    
  <FlowPanel name="Schedule" class="flexible" style="overflow: auto;" />
</FlowPanel>
