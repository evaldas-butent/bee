<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"
  name="DistinctCargoPlaceCompanyLoading" source="CargoLoading"
  filter="Company and Manager = {u}"
  >
  <Columns>
    <SimpleJoin name="LoadingPlace" joinType="INNER">
      <SimpleColumn name="Company" />
    </SimpleJoin>

    <SimpleJoin joinType="INNER" name="Cargo">
      <SimpleJoin joinType="INNER" name="Order">
        <HiddenColumn name="Manager"/>
      </SimpleJoin>
    </SimpleJoin>
  </Columns>
  <GroupBy columns="Company" />
  <Order>
    <OrderBy column="Company" />
  </Order>
</View>