    <Numeric name="Unpacking" precision="15" scale="5" label="=unpacking" />
    <Date name="SupplierTerm" label="=ordSupplierTerm" />
    <Relation name="Parent" relation="OrderItems" label="=taParent" cascade="SET_NULL"/>
