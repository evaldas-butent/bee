    <DataColumn name="Dim04Name" minWidth="100" visible="true" />
  </g:columns>

  <g:columns>
    <DataColumn name="Dim04Background" updateMode="row" minWidth="100" flexGrow="0">
      <editor type="color" />
    </DataColumn>
    <DataColumn name="Dim04Foreground" updateMode="row" minWidth="100" flexGrow="0">
      <editor type="color" />
