<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Table xmlns="http://www.butent.com/table"

  name="Persons"
  idName="PersonID"
  >
  <Fields>
    <String name="FirstName" precision="30" notNull="true" label="=firstName" />
    <String name="LastName" precision="30" label="=lastName" />
    <Date name="DateOfBirth" label="=dateOfBirth" />
    <Relation name="Contact" unique="true" relation="Contacts" editable="true" label="=contacts" />
    <Relation name="PhotoFile" relation="Files" label="=photo"/>
    <Text name="Notes" label="=notes" />
    <Relation name="Activity" relation="CompanyActivities" label="=activity" />
  </Fields>
</Table>
