<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="Contacts"
  idName="ContactID"
  >
  <Fields>
    <String name="Phone" precision="50" label="=phone" />
    <String name="Mobile" precision="50" label="=mobile" />
    <String name="Fax" precision="50" label="=fax" />
    <Relation name="Email" relation="Emails" editable="true" label="=email" />
    <String name="Website" precision="100" label="=website" />
    <String name="Address" precision="100" label="=address" />
    <String name="PostIndex" precision="20" label="=postIndex" />
    <String name="SocialContacts" precision="100" label="=socialContacts" />
    <String name="Notes" precision="255" label="=notes" />
    <Relation name="City" relation="Cities" label="=city" />
    <Relation name="Country" relation="Countries" label="=country" />
  </Fields>
</Table>
