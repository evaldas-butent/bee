<?xml version="1.0" encoding="UTF-8"?>
<View xmlns="http://www.butent.com/view"

      name="CarBundleJobs" source="CarBundleJobs"
      newRowCaption="=newServiceJob" caption="=serviceJobs+(+=bundle+)"
>
  <Columns>
    <SimpleColumn name="Bundle" />

    <SimpleColumn name="Job" />
    <SimpleJoin joinType="LEFT" name="Job">
      <SimpleColumn name="JobName" />
      <SimpleColumn name="Code" />
    </SimpleJoin>

    <SimpleColumn name="Duration" />

    <SimpleColumn name="Quantity" />
    <SimpleColumn name="Price" />

    <SimpleColumn name="Currency" />
    <SimpleJoin joinType="LEFT" name="Currency">
      <SimpleColumn name="Name" alias="CurrencyName" />
    </SimpleJoin>
  </Columns>
</View>
