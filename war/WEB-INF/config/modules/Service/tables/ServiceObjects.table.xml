<?xml version="1.0" encoding="UTF-8"?>
<Table xmlns="http://www.butent.com/table"

  name="ServiceObjects"
  idName="ObjectID"
  >
  <Fields>
    <Relation name="Category" relation="ServiceTree" notNull="true" label="=category" />
    <String name="Address" precision="100" label="=address" />

    <Relation name="Customer" relation="Companies" label="=customer" />
    <Relation name="Contractor" relation="Companies" label="=owner" />

    <Relation name="ContactPerson" relation="CompanyPersons" label="=contact" />
    <Relation name="Object" relation="CompanyObjects" label="=object" />
    <!--TODO: remove Manufacturer deprecated-->
    <Relation name="Manufacturer" relation="Companies" label="=tcdBrand" />
    <Relation name="ServiceManufacturer" relation="TcdManufacturers" label="=tcdBrand" />
    <String name="Model" precision="200" label="=svcModel" />
    <String name="SerialNo" precision="200" label="=svcSerialNo" />

    <!-- Transrifus { -->
    <Long name="ExternalCode" unique="true" label="=externalCode" />
    <String name="YearOfManufacture" precision="100" label="=svcYearOfManufacture" />
    <String name="InventNo" precision="100" label="=svcInventNo"/>
    <String name="BodyNo" precision="100" label="=svcBodyNo"/>
    <String name="Seller" precision="100" label="=svcSeller"/>
    <String name="ItemInButenta" precision="100" label="=svcItemInButenta" />

    <Relation name="Item" relation="Items" unique="true" label="=item" />
    <!-- } Transrifus -->

    <String name="ArticleNo" precision="50" label="=ecItemCode" />
    <Boolean name="ExportWebsite" label="=exportToWebsite" defValue="null" />
    <Date name="AcquireDate" label="=trdAcquireDate" />
    <Text name="Notes" label="=trdNotes" />
  </Fields>
</Table>
